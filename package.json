    "@unhead/schema": "1.8.9",
    "@vue-email/nuxt": "0.8.7",
    "@vuepic/vue-datepicker": "7.4.1",
    "@vueuse/components": "10.7.1",
    "@vueuse/core": "10.7.1",
    "@vueuse/nuxt": "10.7.1",
    "date-fns": "3.1.0",
    "fast-equals": "5.0.1",
    "klona": "2.0.6",
    "maska": "2.1.10",
    "nanoid": "5.0.4",
    "nuxt": "3.9.1",
    "nuxt-headlessui": "1.1.5",
    "nuxt-icon": "0.6.8",
    "nuxt-svgo": "4.0.0",
    "pinia": "2.1.7",
    "postcss-nesting": "12.0.2",
    "swagger-typescript-api": "13.0.3",
    "tailwindcss": "3.4.1",
    "vue": "3.4.6",
    "vue-draggable-next": "2.2.1",
    "vue-router": "4.2.5"
