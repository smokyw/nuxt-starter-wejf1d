    "prepare": "nuxt prepare",
    "prettier": "prettier --check --ignore-unknown --no-error-on-unmatched-pattern *",
    "prettier:write": "prettier --write --ignore-unknown --no-error-on-unmatched-pattern *",
    "preview": "nuxt preview",
    "sta": "swagger-typescript-api -p https://raw.githubusercontent.com/shustov-alex/ipr-box-apidoc/master/openapi.json",
    "typecheck": "nuxt typecheck"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,json}": [
      "eslint --cache --fix",
      "prettier --write -u"
    ]
  },
  "dependencies": {
    "@headlessui/tailwindcss": "0.2.0",
    "@nuxtjs/google-fonts": "3.1.3",
    "@nuxtjs/i18n": "8.0.0",
