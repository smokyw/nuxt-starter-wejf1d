{
  "name": "idp-box-front",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt generate",
    "dev": "nuxt dev",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.json --max-warnings=0",
